{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "unified-signatures": false,
    "no-unused-variable": true,
    "quotemark": [true, "single"]
  }
}
